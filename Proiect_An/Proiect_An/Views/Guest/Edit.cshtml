@model Proiect_An.Models.Guest

<h2>Edit Guest</h2>
<form asp-action="Edit">
    <input type="hidden" asp-for="Id" />
    <div>
        <label>Name</label>
        <input asp-for="Name" />
    </div>
    <div>
        <label>Phone Number</label>
        <input asp-for="PhoneNumber" />
    </div>
    <div>
        <label>Address</label>
        <input asp-for="Address" />
    </div>
    <button type="submit">Save</button>
</form>
