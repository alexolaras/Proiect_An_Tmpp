<h3>Index</h3>

<table class="table">
    <thead>
        <tr>
            <th>Guest Name</th>
            <th>Check In Date</th>
            <th>Check Out Date</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.GuesName</td>
                <td>@item.CheckIn</td>
                <td>@item.CheckOut</td>
                @*<td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a>
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>*@
            </tr>
        }
    </tbody>
</table>